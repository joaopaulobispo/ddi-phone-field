{
    "name": "wplugin/ddi-phone-field",
    "description": "WordPress plugin that adds international phone fields with DDI and country flags to Elementor, Contact Form 7 and WooCommerce forms.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Wplugin",
            "email": "contato@wplugin.com.br",
            "homepage": "https://www.wplugin.com.br"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.6",
        "wp-coding-standards/wpcs": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "DDI_Phone_Field\\": "includes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DDI_Phone_Field\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs --standard=WordPress",
        "phpcbf": "phpcbf --standard=WordPress"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
